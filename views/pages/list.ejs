<% layout('../layouts/list') -%>
    <div class="row">
        <div class="col-12">
            <div class="page-title-box">
                <div class="page-title-left">
                    <ol class="breadcrumb m-0">
                        <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
                        <li class="breadcrumb-item"><a href="javascript:
                                void(0);">Pages</a></li>
                        <li class="breadcrumb-item active"><a href="javascript:
                                void(0);">List</a></li>
                    </ol>
                </div>
            </div>
        </div>
        <div class="col-sm-12">
            <div class="page-title-box">
                <h4 class="page-title">Pages List</h4>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <table class="table nowrap" id="pages-datatable">
                        <thead>
                            <tr>
                                <th>S.NO.</th>
                                <th>Title</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                    <div class="row tableLoader">
                        <div class="col-sm-12 text-center">
                            <div class="spinner-border avatar-md text-primary
                                m-2" role="status"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
